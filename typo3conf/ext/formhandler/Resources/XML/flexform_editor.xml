<T3DataStructure>
	<meta>
	<langDisable>1</langDisable>
	<!--  <langChildren>1</langChildren>!-->
		</meta>
<sheets>
	<sDEF>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.sheet_general</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<predefined>
				<TCEforms>
					<label>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.template_predefined</label>
					<config>
					    <type>select</type>
						<itemsProcFunc>tx_dynaflex_formhandler->addFields_predefined</itemsProcFunc>
					    <maxitems>1</maxitems>
					    <size>1</size>
					    <multiple>0</multiple>
					    <selectedListStyle>width:150px</selectedListStyle>
					    <itemListStyle>width:150px</itemListStyle>
					</config>
				</TCEforms>
			</predefined>
			<predefinedJS>
				<TCEforms>
					<config>
					    <type>user</type>
						<userFunc>tx_dynaflex_formhandler->addFields_predefinedJS</userFunc>
					</config>
				</TCEforms>
			</predefinedJS>
		</el>
	</ROOT>
	</sDEF>
	<sEMAILUSER>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.sheet_emailUserSettings</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<to_email>
					<TCEforms>
						<label>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.emailConfirm</label>
						<config>
							<type>input</type>
							<eval>text</eval>
							<size>50</size>
						</config>
					</TCEforms>
				</to_email>
				<subject>
					<TCEforms>
						<label>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.emailConfirmSubject</label>
						<config>
							<type>input</type>
							<eval>text</eval>
							<size>50</size>
						</config>
					</TCEforms>
				</subject>
				<sender_email>
					<TCEforms>
						<label>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.emailFrom</label>
						<config>
							<type>input</type>
							<eval>text</eval>
							<size>50</size>
						</config>
					</TCEforms>
				</sender_email>
			</el>
		</ROOT>
	</sEMAILUSER>
	<sMISC>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:formhandler/Resources/Language/locallang_tca.php:formhandler.pi_flexform.sheet_miscSettings</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<redirect_page>
					<TCEforms>
						<label>LLL:EXT:formhandler/Resources/Language/locallang_tca.xml:formhandler.pi_flexform.redirectPage</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</redirect_page>
			</el>
		</ROOT>
	</sMISC>
</sheets>
</T3DataStructure>